    "postcss-loader": "2.0.6",
    "precss": "2.0.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "3.0.0-beta.7",
    "sinon": "^2.4.1",
    "style-loader": "0.18.2",
    "svg-icons": "1.0.13",

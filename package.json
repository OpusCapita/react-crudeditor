{
  "name": "@opuscapita/react-crudeditor",
  "version": "1.3.2",
  "description": "React.js CRUD Editor",
  "repository": "OpusCapita/react-crudeditor",
  "main": "lib/index.js",
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss",
    "*.sass"
  ],
  "scripts": {
    "lint": "eslint ./src --config ./config/.eslintrc",
    "lint-fix": "eslint ./src --config ./config/.eslintrc --fix",
    "test": "rimraf ./.nyc_output ./coverage && cross-env NODE_ENV=test nyc mocha --recursive \"src/**/*.spec.js\" --require ignore-styles",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.client.js",
    "start-showroom": "showroom-scan ./src && cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.showroom.js",
    "build": "rimraf lib && webpack --config ./config/webpack.config.build.js",
    "publish-release": "npm run build && npm publish"
  },
  "keywords": [
    "react",
    "crud",
    "editor",
    "webpack"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "browserslist": [
    "chrome >= 64",
    "firefox ESR",
    "ie >= 11",
    "safari >= 11"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@opuscapita/react-dates": "0.4.2",
    "@opuscapita/react-svg": "4.0.0",
    "babel-runtime": "6.26.0",
    "big.js": "5.0.3",
    "lodash": "4.17.10",
    "object-hash": "1.3.0",
    "prop-types": "15.6.1",
    "react-bootstrap": "0.31.5",
    "react-notifications": "1.4.3",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-saga": "1.0.0-beta.0",
    "updeep": "1.0.0"
  },
  "peerDependencies": {
    "react": "^15.6.2 || ^16.2.0",
    "react-dom": "^15.6.2 || ^16.2.0"
  },
  "devDependencies": {
    "@opuscapita/i18n": "1.2.4",
    "@opuscapita/npm-scripts": "2.0.1-beta.1",
    "@opuscapita/react-navigation": "1.0.11",
    "@opuscapita/react-reference-select": "2.1.2",
    "@opuscapita/react-showroom-client": "1.3.0-beta.7",
    "@opuscapita/react-showroom-server": "1.3.1",
    "@opuscapita/styles": "1.1.24",
    "autoprefixer": "8.3.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-import": "1.7.0",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "bootstrap-sass": "3.3.7",
    "chai": "4.1.2",
    "core-js": "2.5.5",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-15": "1.0.5",
    "eslint": "4.19.1",
    "eslint-config-opuscapita": "2.0.0",
    "eslint-plugin-react": "7.7.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "ignore-styles": "5.0.1",
    "joi-browser": "13.0.1",
    "jsdom": "11.9.0",
    "less-loader": "4.1.0",
    "mocha": "5.1.1",
    "mocha-junit-reporter": "1.17.0",
    "nyc": "11.7.1",
    "object.entries": "1.0.4",
    "postcss-loader": "2.1.4",
    "precss": "3.1.2",
    "query-string": "5.0.0",
    "raw-loader": "0.5.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "15.6.2",
    "regenerator-runtime": "0.11.1",
    "rimraf": "2.6.2",
    "sinon": "4.5.0",
    "style-loader": "0.21.0",
    "svg-icons": "1.0.13",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-dev-server": "3.1.3",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.7.2"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "src/external_modules",
      "node_modules",
      "src/**/*.spec.js",
      "src/crudeditor-lib/i18n"
    ],
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "./config/mocha-config.js"
    ],
    "statements": 50,
    "branches": 40,
    "functions": 40,
    "lines": 50
  }
}

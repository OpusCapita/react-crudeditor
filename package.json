    "@opuscapita/react-showroom-server": "^1.3.0",
    "@opuscapita/styles": "1.0.0",
    "autoprefixer": "7.1.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",

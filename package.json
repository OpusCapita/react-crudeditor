    "sass-loader": "6.0.6",
    "sinon": "^4.0.1",
    "style-loader": "0.18.2",
    "svg-icons": "1.0.13",
    "url-loader": "0.5.9",
    "webpack": "3.4.1",
    "webpack-dev-server": "^2.9.2"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "src/external_modules",
      "node_modules"
    ],
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "./config/mocha-config.js"
    ],
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80
  }

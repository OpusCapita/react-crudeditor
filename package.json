{
  "name": "@opuscapita/react-crudeditor",
  "version": "0.0.16",
  "description": "React.js CRUD Editor",
  "repository": "OpusCapita/react-crudeditor",
  "main": "lib/index.js",
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src --config ./config/.eslintrc",
    "lint-fix": "node ./node_modules/eslint/bin/eslint.js ./src --config ./config/.eslintrc --fix",
    "test": "rimraf ./.nyc_output ./coverage && cross-env nyc mocha --recursive src/**/*.spec.js --require ignore-styles",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.babel.js",
    "build": "cross-env NODE_ENV=development webpack --config config/webpack.config.babel.js",
    "start-showroom": "cross-env NODE_ENV=development showroom-scan ./src/demo/showroom && webpack-dev-server --config config/webpack.config.showroom.js",
    "build-showroom": "cross-env NODE_ENV=development showroom-scan ./src/demo/showroom && webpack --config config/webpack.config.showroom.js",
    "npm-build": "rimraf lib && cross-env NODE_ENV=production webpack --config ./config/webpack.config.build.js",
    "npm-publish": "update-changelog -- --all && npm run npm-build && npm-publish",
    "publish-release": "npm run npm-publish -- --release"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@opuscapita/react-dates": "0.3.0-beta.16",
    "@opuscapita/react-svg": "2.0.0",
    "base-64": "0.1.0",
    "big.js": "3.1.3",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react-bootstrap": "0.31.1",
    "react-notifications": "1.4.3",
    "react-redux": "5.0.5",
    "redux": "3.7.2",
    "redux-saga": "0.15.6",
    "updeep": "1.0.0"
  },
  "peerDependencies": {
    "react": ">= 0.14.0",
    "react-dom": ">= 0.14.0"
  },
  "devDependencies": {
    "@opuscapita/i18n": "1.1.0",

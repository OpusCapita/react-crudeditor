{
  "name": "react-crud",
  "version": "0.0.1",
  "private": true,
  "description": "React.js CRUD Editor",
  "repository": "OpusCapita/react-crudeditor",
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src --config ./config/.eslintrc --fix",
    "test": "rimraf ./.nyc_output ./coverage && cross-env nyc mocha --recursive src/**/*.spec.js  --require ignore-styles",
    "start": "webpack-dev-server --config config/webpack.config.babel.js",
    "build": "webpack --config config/webpack.config.babel.js",
    "start-showroom": "showroom-scan ./src/demo/showroom && webpack-dev-server --config config/webpack.config.showroom.js",
    "build-showroom": "showroom-scan ./src/demo/showroom && webpack --config config/webpack.config.showroom.js"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "a.ryashentsev",
  "license": "ISC",
  "dependencies": {
    "@opuscapita/i18n": "^1.1.0",
    "@opuscapita/react-dates": "^0.3.0-beta.13",
    "@opuscapita/react-svg": "2.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-runtime": "6.25.0",
    "big.js": "3.1.3",
    "body-parser": "1.17.2",
    "express": "4.15.3",
    "lodash": "4.17.4",
    "prop-types": "^15.5.10",
    "query-string": "5.0.0",
    "react": "15.6.1",
    "react-bootstrap": "0.31.1",
    "react-dom": "15.6.1",
    "react-notifications": "^1.4.3",
    "react-redux": "5.0.5",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
    "redux": "3.7.2",
    "redux-saga": "0.15.6",
    "superagent": "3.5.2",
    "updeep": "1.0.0"

  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "src/external_modules",
      "node_modules"
    ],
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "require": [
      "./config/mocha-config.js"
    ],
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80
  }

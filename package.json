{
  "name": "react-crud",
  "version": "1.0.0",
  "private": true,
  "description": "react.js CRUD Editor",
  "scripts": {
    "start": "cross-env NODE_ENV=development && nodemon --watch src/server src/server --exec babel-node --presets es2015,es2016,es2017,stage-0",
    "test": "node ./node_modules/jest/bin/jest.js --no-cache",
    "test:watch": "npm test -- --watch",
    "babel-node": "node ./node_modules/babel-cli/bin/babel-node.js --presets es2015,es2016,es2017,stage-0",
    "serve:watch": "cross-env NODE_ENV=development && nodemon --exec yarn babel-node --watch ./src ./src/server/index.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src --fix",
    "test:mocha": "rimraf ./.nyc_output ./coverage && cross-env nyc mocha --recursive src/**/*.spec.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "a.ryashentsev",
  "license": "ISC",
  "dependencies": {
    "@opuscapita/react-svg": "2.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-runtime": "6.25.0",
    "big.js": "3.1.3",
    "body-parser": "1.17.2",
    "express": "4.15.3",
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "query-string": "5.0.0",
    "react": "15.6.1",
    "react-bootstrap": "0.31.1",
    "react-dom": "15.6.1",
    "react-redux": "5.0.5",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
    "redux": "3.7.2",
    "redux-saga": "0.15.6",
    "superagent": "3.5.2",
    "updeep": "1.0.0"
  },
  "devDependencies": {
    "@opuscapita/react-navigation": "0.0.20",
    "@opuscapita/styles": "1.0.0",
    "autoprefixer": "7.1.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bootstrap-sass": "3.3.7",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "css-loader": "0.28.4",
    "eslint": "^4.8.0",
    "eslint-config-opuscapita": "^1.0.9",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "joi-browser": "10.6.1",
    "less-loader": "4.0.5",
    "lokijs": "1.5.0",
    "mocha": "^4.0.1",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "nyc": "^11.2.1",
    "postcss-loader": "2.0.6",
    "precss": "2.0.0",
    "raw-loader": "0.5.1",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.2",
    "sass-loader": "6.0.6",
    "sinon": "2.4.1",
    "style-loader": "0.18.2",
    "svg-icons": "1.0.13",
    "url-loader": "0.5.9",
    "webpack": "3.4.1",
    "webpack-dev-middleware": "1.11.0"
  }
}
